    <validationRules>
        <fullName>Event_Future_Date_Rule</fullName>
        <active>true</active>
        <description>Attendee can only be associated with the Event whose End Date is in future</description>
        <errorConditionFormula>AND(Event__r.End__c &lt; NOW(), NOT(ISNULL(Event__r.End__c)))</errorConditionFormula>
        <errorMessage>Attendee can only be associated with the Event whose End Date is in future</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Event_Live_Acceptance_Rule</fullName>
        <active>true</active>
        <description>Attendee can only be associated with the Event whose End Date is in future &amp; Event Live Checkbox is checked and Event is accepting the Attendees ( means Remaining Seats field value is not 0 )</description>
        <errorConditionFormula>OR(AND( Event__r.Remaining_Seats__c = 0, NOT(ISBLANK(Attendee__r.Name))), Event__r.Live__c = FALSE)</errorConditionFormula>
        <errorDisplayField>Event__c</errorDisplayField>
        <errorMessage>Event is not Live or is not accepting any more attendees</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
