    <validationRules>
        <fullName>End_date_must_be_1_day_ahead_of_Start_da</fullName>
        <active>true</active>
        <description>End Date/Time must be at-least 1 day ahead of Start Date/Time ( If there is a value in End Date/Time field)</description>
        <errorConditionFormula>AND(DATEVALUE( End__c ) &lt; (DATEVALUE( Start__c ) + 1), NOT( ISNULL(End__c) ) )</errorConditionFormula>
        <errorDisplayField>End__c</errorDisplayField>
        <errorMessage>End Date/Time must be at-least 1 day ahead of Start Date/Time</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>InPerson_Event_Type_Rule</fullName>
        <active>true</active>
        <description>If Event Type field value is In-Person then user must need to select Location on Event Record.</description>
        <errorConditionFormula>AND( ISPICKVAL( Event_Type__c , &apos;In-person&apos;) ,   (ISBLANK(Location_Address_Book__c ) ) )</errorConditionFormula>
        <errorMessage>If Event Type field value is In-Person then user must need to select Location on Event Record.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Recurring_Checkbox_Rule</fullName>
        <active>true</active>
        <description>1. If Recurring? checkbox is checked then user must need to fill Frequency field &amp; If checkbox is unchecked then User can not select Frequency field.</description>
        <errorConditionFormula>OR(AND(Recurring__c = TRUE, ISBLANK( TEXT( Frequency__c ))),AND(Recurring__c  = FALSE , NOT(ISBLANK( TEXT( Frequency__c )))))</errorConditionFormula>
        <errorDisplayField>Frequency__c</errorDisplayField>
        <errorMessage>If Recurring? checkbox is checked then user must need to fill Frequency field</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Virtual_Event_Type_Rule</fullName>
        <active>true</active>
        <description>If Virtual is Selected as Value for Event Type field then Prevent User to Select Location on Event Record.</description>
        <errorConditionFormula>AND( ISPICKVAL( Event_Type__c , &apos;Virtual&apos;) ,   NOT(ISBLANK(Location_Address_Book__c ) ) )</errorConditionFormula>
        <errorMessage>If Virtual is Selected as Value for Event Type field then Prevent User to Select Location on Event Record.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
