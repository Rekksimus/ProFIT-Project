    <fields>
        <fullName>Status_Indicator__c</fullName>
        <description>Formula for displaying red flag on a event with deleted status</description>
        <externalId>false</externalId>
        <formula>IF( ISPICKVAL( Status__c , &apos;Deleted&apos;) , (IMAGE(&quot;/servlet/servlet.FileDownload?file=0150l000000oxN8&quot;,&quot;Red Flag&quot;, 100,100)),null)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Status Indicator</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
